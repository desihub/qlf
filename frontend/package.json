{
  "name": "qlf-ui",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.js",
  "build": {
    "extends": null,
    "appId": "br.gov.linea.qlf",
    "mac": {
      "category": "public.app-category.education"
    }
  },
  "dependencies": {
    "@material-ui/core": "3.9.2",
    "create-react-app": "^1.5.2",
    "dotenv": "^4.0.0",
    "halogenium": "^2.2.3",
    "history": "^4.7.2",
    "material-ui": "^0.19.4",
    "material-ui-icons": "^1.0.0-beta.17",
    "moment": "^2.19.2",
    "re-resizable": "^4.9.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-grid-layout": "^0.16.0",
    "react-iframe": "^1.1.0",
    "react-recaptcha": "^2.3.7",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.8",
    "react-scripts": "1.0.16",
    "react-tooltip": "^3.4.0",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "serve": "9.1.0",
    "util.promisify": "^1.0.0",
    "victory": "^0.24.2"
  },
  "scripts": {
    "coverage": "jest --coverage --setupTestFrameworkScriptFile=raf/polyfill",
    "lint-fix": "eslint --fix .",
    "lint": "eslint .",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --setupTestFrameworkScriptFile=raf/polyfill",
    "watch": "jest --watch --setupTestFrameworkScriptFile=raf/polyfill",
    "eject": "react-scripts eject",
    "serve": "serve -s build -l $QLF_UI_PORT"
  },
  "jest": {
    "testURL": "http://qlf-ui.com/",
    "setupFiles": [
      "./setup-jest.js"
    ],
    "moduleNameMapper": {
      ".+\\.(mp3|png)$": "jest-transform-stub"
    },
    "modulePathIgnorePatterns": [
      "npm-cache",
      ".npm"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "^2.8.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^21.2.1",
    "jest-fetch-mock": "^1.5.0",
    "jest-transform-stub": "^1.0.0",
    "mock-socket": "^7.1.0",
    "prettier": "1.8.2",
    "react-test-renderer": "^16.1.1",
    "sinon": "^4.1.2"
  }
}
